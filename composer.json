{
    "name": "datashaman/elastic-model",
    "description": "Laravel/Eloquent integration with Elasticsearch",
    "keywords": ["elasticsearch"],
    "license": "Apache-2.0",
    "type": "library",
    "require": {
        "php": ">=5.5.9",
        "elasticsearch/elasticsearch": "~2.0@beta"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1"
    },
    "autoload": {
        "psr-4": {
            "Datashaman\\ElasticModel\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
    },
    "config": {
        "preferred-install": "dist"
    }
}
