{
    "name": "datashaman/elastic-model",
    "description": "Laravel/Eloquent integration with Elasticsearch",
    "keywords": ["elasticsearch"],
    "license": "Apache-2.0",
    "type": "library",
    "authors": [
        {
            "name": "Marlin Forbes",
            "email": "marlinf@datashaman.com"
        }
    ],
    "require": {
        "php": "^5.5.9 || ^7.0",
        "illuminate/support": "5.1.* || 5.2.* || 5.3.*",
        "elasticsearch/elasticsearch": "^2.2",
        "mockery/mockery": "^0.9.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8 || ^5.0",
        "orchestra/testbench": "^3.2",
        "codeception/aspect-mock": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Datashaman\\ElasticModel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Datashaman\\ElasticModel\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "coverage": "phpunit --coverage-html reports"
    },
    "config": {
        "preferred-install": "dist"
    }
}
